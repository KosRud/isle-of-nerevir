{
	"applicationId": "js.neutralino.zero",
	"version": "1.0.0",
	"defaultMode": "window",
	"documentRoot": "/vue-src/dist/",
	"url": "/",
	"enableServer": true,
	"enableNativeAPI": true,
	"nativeAllowList": ["app.*", "debug.*"],
	"modes": {
		"window": {
			"title": "Isle of Nerevir",
			"width": 800,
			"height": 500,
			"minWidth": 400,
			"minHeight": 200,
			"icon": ""
		}
	},
	"cli": {
		"binaryName": "Isle of Nerevir",
		"resourcesPath": "/vue-src/dist/",
		"extensionsPath": "/extensions/",
		"binaryVersion": "5.5.0",
		"clientVersion": "5.5.0",
		"frontendLibrary": {
			"patchFile": "/vue-src/index.html",
			"devUrl": "http://localhost:5173",
			"projectPath": "/vue-src/",
			"initCommand": "npx pnpm install",
			"devCommand": "npm run dev",
			"buildCommand": "npm run build"
		}
	}
}
